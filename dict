siswa = {
    'name': 'Wijaya Kusuma',
    'age': 20,
    'awesome': True,
    'language': ['C', 'Python', 'SQL'],
    'address' : 'Pontianak - Kalimantan Barat',
    'favorites': {
        'food': 'Indomie',
        'sport': 'Balap Karung'
    }
}

name = siswa.get('name')
print(name)

hobby = siswa.get('favorites')['food']
print(hobby)

address = siswa.get('address')
print(address)

for key, val in siswa.items():
    print(key, '=', siswa[key])
